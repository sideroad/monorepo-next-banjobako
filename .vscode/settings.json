{
  //    "editor.formatOnSave": false,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
      "javascript",
      "javascriptreact",
      {"language": "typescript", "autoFix": true },
      {"language": "typescriptreact", "autoFix": true }
  ],
  "window.zoomLevel": 2,
  "editor.tabSize": 2,
  "workbench.tree.indent": 18,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}