{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "reducers": "./reducers",
          "types": "./../../types"
        },
        "extensions": [".js", ".jsx", ".tsx", "ts"]
      }
    ]
  ]
}